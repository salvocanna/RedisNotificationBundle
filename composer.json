{
  "name": "salvocanna/redis-notification-bundle",
  "type" : "symfony-bundle",
  "description": "Symfony Bundle for Redis Based Notifications",
  "keywords": ["symfony redis notification bundle"],
  "license": "MIT",
  "authors": [{
    "name" : "Salvo Canna",
    "email" : "salvocanna@users.noreply.github.com"
  }],
  "require" : {
    "php": ">=5.4",
    "symfony/form": "^2.7 || ^3.0",
    "symfony/framework-bundle": "^2.7 || ^3.0",
    "symfony/twig-bundle": "^2.7 || ^3.0",
    "twig/twig": "^1.28 || ^2.0",
    "symfony/security-bundle": "^2.7 || ^3.0",
    "symfony/templating": "^2.7 || ^3.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^1.11",
    "symfony/console": "^2.7 || ^3.0",
    "symfony/phpunit-bridge": "^2.7 || ^3.0",
    "symfony/validator": "^2.7 || ^3.0",
    "symfony/yaml": "^2.7 || ^3.0",
    "phpunit/phpunit": "~4.8|~5.0"
  },
  "autoload" : {
    "psr-4": {
      "SalvoCanna\\RedisNotificationBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0-dev"
    }
  }
}
